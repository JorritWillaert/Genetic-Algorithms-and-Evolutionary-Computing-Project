isbn = {978-1-4377-4461-3},
pages = {49--81},
publisher = {William Andrew Publishing},
title = {{5 - Material Surface Preparation Techniques}},
url = {http://www.sciencedirect.com/science/article/pii/B9781437744613100057},
year = {2011}
}
